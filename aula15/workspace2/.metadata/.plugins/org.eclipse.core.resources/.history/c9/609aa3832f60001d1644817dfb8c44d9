package exemplos;

public class Main1 {

	public static void main(String[] args) {
		String aluno = "Juquinha";
		float prova = 7.5f;
		float trabalho = 4.5f;
		float atividades = 2;
		float media = (prova + trabalho + atividades) / 3;

		System.out.println(aluno +"" prova + ""trabalho + atividades"" + media"");
	}

}
